{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "name": "Web You",
  "description": "Penis Galore",
  "version": "1.0",


  "background": {
    "scripts": ["thirdparty/jquery-2.1.4.min.js","background.js"]
  },
  "chrome_url_overrides" : {
     "newtab": "webyou.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js", "webyou.js"]
    }
  ],
  "permissions": [
    "idle",
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ]
}